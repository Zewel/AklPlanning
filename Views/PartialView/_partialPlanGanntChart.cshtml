@model System.Data.DataTable;
@using System.Data;
<div class="card card-primary" style=" width: 100%">
    <div class="card-header">
        <div class="row">
            <div class="col-md-4">
                <h3 class="card-title"><span class="fa fa-list"> Style List</span></h3>
            </div>
            <div class="col-md-4">
                @*<label class="label-info">Total Order Qty is:@Model.AsEnumerable().Sum(x => Convert.ToInt32(x.ItemArray[3])) </label>*@
            </div>
            <div class="col-md-4">
                <button class="button btn-info" style="float:right" onclick="ExportAllData_HTML()"> <span class=" fa fa-list"></span>Excel</button>
            </div>
        </div>

    </div>
    <!-- /.card-header -->
    <!-- form start -->
    <form>
        <div class="card-body table-responsive " style="height:280px">
            <table class="table  table-bordered table-head-fixed text-nowrap table-hover" id="exportStylelList">
                <thead style="background-color:aliceblue">
                    <tr>
                        <th>Buyer</th>
                        <th>Booking Gauge</th>
                        <th>Style</th>
                        <th>PONo</th>
                        <th>Booked POQty</th>
                        <th>ExftyNew</th>
                        <th>Factory</th>
                        <th>Brand</th>
                        <th>Production Gauge</th>
                        <th>Plan PoQty</th>
                        <th>StartDate</th>
                        <th>EndDate</th>

                    </tr>
                </thead>
                <tbody>
                    @foreach (DataRow item in Model.Rows)
                    {
                        <tr>
                            <td>@item.ItemArray[0]</td>
                            <td>@item.ItemArray[1]</td>
                            <td>@item.ItemArray[2]</td>
                            <td>@item.ItemArray[4]</td>
                            <td>@item.ItemArray[5]</td>
                            <td>@item.ItemArray[6]</td>
                            <td>@item.ItemArray[8]</td>
                            <td>@item.ItemArray[9]</td>
                            <td>@item.ItemArray[10]</td>
                            <td>@item.ItemArray[12]</td>
                            <td>@item.ItemArray[13]</td>
                            <td>@item.ItemArray[14]</td>
                        </tr>


                    }




                </tbody>
            </table>

        </div>
    </form>

</div>

